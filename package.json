{
  "name": "myinvestmentapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --config jest.config.js",
    "test:e2e:android": "npm run detox:build:android:debug && npm run detox:test:android:debug",
    "test:e2e:ios": "npm run detox:build:ios:debug && npm run detox:test:ios:debug",
    "lint": "expo lint",
    "detox:build:android:debug": "detox build --configuration android.emu.debug",
    "detox:build:ios:debug": "npx pod-install && detox build --configuration ios.sim.debug",
    "detox:test:android:debug": "detox test --configuration android.emu.debug",
    "detox:test:ios:debug": "detox test --configuration ios.sim.debug",
    "detox:build:android": "detox build --configuration android.emu.release",
    "detox:build:ios": "npx pod-install && detox build --configuration ios.sim.release",    
    "detox:test:android": "detox test --configuration android.emu.release",
    "detox:test:ios": "detox test --configuration ios.sim.release"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.3",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/material-bottom-tabs": "^6.2.29",
    "@react-navigation/native": "^6.1.18",
    "@types/react": "~18.2.79",
    "@types/react-native": "^0.72.8",
    "@types/react-navigation": "^3.0.8",
    "expo": "~51.0.28",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.10",
    "typescript": "~5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "detox": "^20.27.4",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
